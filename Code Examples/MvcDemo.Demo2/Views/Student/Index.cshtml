@using MvcDemo.Demo2.Models
@model IEnumerable<MvcDemo.Demo2.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>Name</th>
        <th>Phone</th>
        <th>Address</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@item.Name</td>
        <td>@item.Phone</td>
        <td>@item.Address</td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Id })
        </td>
    </tr>
}

</table>
@*This is using TempData to show last student viewed*@
@if (ViewBag.Last != null)
{
    <strong>Last Viewd: @ViewBag.Last.Name</strong>
}